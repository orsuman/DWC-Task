<div class="container">
 <div class="row">

   <div class="col-xs-3">
   <%= render 'books/new', book: @book, user: @user %>
   </div>

   <div class="col-xs-9">
        <h2>Users</h2>
            <table class="table">
              <tr><th></th> <th>name</th> <th></th></tr>
                <% @users.each do |user| %>
                 <tr align="left"><td><%= attachment_image_tag user, :profile_image, :fill, 45, 45,fallback: "no_image.jpg",size:'45x45' %></td><td><%= user.name %></td><td><%= link_to "Show",user_path(user.id) %></td>
              </tr>
                <% end %>
            </table>
    </div>

 </div>
</div>